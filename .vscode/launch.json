  "version": "0.2.0",
  "configurations": [
    {
      "name": "backend",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/backend/api/main.py",
      "cwd": "${workspaceFolder}/backend/api",
      "python": "${workspaceFolder}/backend/api/env/bin/python"
    },

    {
      "name": "frontend",
      "cwd": "frontend",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "frontend web",
      "cwd": "frontend",
      "request": "launch",
      "type": "dart",
      "args": ["-d", "chrome", "--web-port", "3000"]
    }
  ]
}
